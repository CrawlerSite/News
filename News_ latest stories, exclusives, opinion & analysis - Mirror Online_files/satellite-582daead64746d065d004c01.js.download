_satellite.pushAsyncScript(function(event, target, $variables){
  <!-- Chartbeat Tag -->
var _sf_async_config = {},
    subSection = _satellite.getVar("Collect_SubSectionTwo") ? ',' +  _satellite.getVar("Collect_SubSectionTwo"): '',
    authors = _satellite.getVar("Collect_Author");

window._sf_startpt = _satellite.getVar("Collect_NavigationStart");
/** CONFIGURATION START **/
_sf_async_config.uid = 33715;
_sf_async_config.domain = _satellite.getVar("Collect_OwnerSiteDomain");
_sf_async_config.useCanonical = true;
_sf_async_config.sections = _satellite.getVar("Collect_Channel") + subSection;
_sf_async_config.authors = (authors.indexOf('|') > -1) ? authors.replace('|', ',') : authors;

window._sf_async_config = _sf_async_config;
/** CONFIGURATION END **/
(function() {
  function loadChartbeat() {
    window._sf_endpt = (new Date()).getTime();
    var e = document.createElement('script');
    e.setAttribute('language', 'javascript');
    e.setAttribute('type', 'text/javascript');
    e.setAttribute('src', '//static.chartbeat.com/js/chartbeat.js');
    document.body.appendChild(e);
  }
  loadChartbeat();
})(); 
<!-- Chartbeat Tag End -->
});
